{
    "name": "jtc-solutions/code-generator",
    "description": "Package that helps generating CRUD, services and repository boilerplate.",
    "type": "symfony-bundle",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "JtcSolutions\\CodeGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JtcSolutions\\CodeGenerator\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "David Jungman",
            "email": "david@jtc-solutions.cz"
        }
    ],
    "require": {
        "ramsey/uuid": "^4.7",
        "php": ">=8.3",
        "symfony/console": "^7.2",
        "jtc-solutions/helpers": "^0.2",
        "symfony/http-foundation": "^7.2",
        "nelmio/api-doc-bundle": "^5.0",
        "nikic/php-parser": "^5.0",
        "symfony/filesystem": "^7.2",
        "jtc-solutions/core": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^2.1",
        "grifart/phpstan-oneline": "^0.5.0",
        "symplify/easy-coding-standard": "^12.1",
        "slevomat/coding-standard": "^8.16",
        "mikey179/vfsstream": "^1.6"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
